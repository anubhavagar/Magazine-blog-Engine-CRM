@model AapkiMagazine.Models.PageNotFound
<div id="content" class="content section row">
    <div class="col-md-4 col-lg-4 col-xl-3">
    
        <h2>
            @Model.Error
        </h2>
        <hr />
        <p>
            @Model.Message</p>
          
            <a href="@Url.Action("Index", "Home")" rel="bookmark"><i class="icon-home"></i> GO HOME</a>
    </div>
    <div class="col-md-8 col-lg-8 col-xl-9">
       <ul id="scene" class="scene">
            <li class="layer" data-depth="1.00">
                <img src="../../Content/images/layers/kidmondo_face_sad.gif" /></li>           
        </ul>
    </div>
</div>
<!-- Scripts -->
<script src="../../Scripts/js/jquery.parallax.min.js" type="text/javascript"></script>
<script type="text/javascript">
    $(function () { $('#scene').parallax(); });

</script>
